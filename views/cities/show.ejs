<div class="container">
<h1> <%= location.city %></h1>
<img src="<%= location.pic %>"/>
<div class="card blue-grey darken-1">
        <% location.posts.forEach (p => { %>
    <div class="card-content white-text">
      <span class="card-title"><%= p.title %></span>
    </div>
    <div class="card-action">
      <a href="#">read post</a>
    </div>
  </div>  
</div>
<% }) %>
</div>
</div>


          