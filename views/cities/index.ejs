<div class="container">
<div class="row">
        <div class="col s12 m6">
          <div class="card">
                <% payload.forEach(p => { %>
            <div class="card-image">
              <img src="<%= p.pic %>">
              <span class="card-title"><%= p.city %></span>
              <a class="btn-floating halfway-fab waves-effect waves-light red"><i class="material-icons">+</i></a>
            </div>
            <div class="card-content">
                <span class="card-title activator grey-text text-darken-4"><i class="material-icons left">more info</i><br /></span>
            </div>
            <div class="card-reveal">
                <span class="card-title grey-text text-darken-4"><%= p.city %><i class="material-icons right">close</i></span>
                <p>New York City is the biggest city in the United States and the State of New York.</p>
            </div>
          </div>
          <% }) %>
        </div>
    </div>
</div>